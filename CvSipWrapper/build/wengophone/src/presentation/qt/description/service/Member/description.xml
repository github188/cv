<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
   <specVersion>
      <major>1</major>
      <minor>0</minor>
   </specVersion>
   <actionList>
      <action>
         <name>ChooseMember</name>
         <argumentList>
            <argument>
               <name>MemberName</name>
               <direction>in</direction>
               <relatedStateVariable>memberName</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>ContactMember</name>
         <argumentList>
            <argument>
               <name>MemberName</name>
               <direction>in</direction>
               <relatedStateVariable>memberName</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>StopMember</name>
         <argumentList>
            <argument>
               <name>MemberName</name>
               <direction>in</direction>
               <relatedStateVariable>memberName</relatedStateVariable>
            </argument>
         </argumentList>
      </action>      
      <action>
         <name>GetMemberList</name>
         <argumentList>
            <argument>
               <name>MemberList</name>
               <direction>out</direction>
               <relatedStateVariable>memberList</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>InviteMember</name>
         <argumentList>
            <argument>
               <name>UserAccount</name>
               <direction>in</direction>
               <relatedStateVariable>userAccount</relatedStateVariable>
            </argument>
         </argumentList>
      </action>  
      <action>
         <name>AcceptInvite</name>
         <argumentList>
            <argument>
               <name>IsAccept</name>
               <direction>in</direction>
               <relatedStateVariable>isAccept</relatedStateVariable>
            </argument>
         </argumentList>
      </action>     
      <action>
         <name>KickMember</name>
         <argumentList>
            <argument>
               <name>MemberName</name>
               <direction>in</direction>
               <relatedStateVariable>memberName</relatedStateVariable>
            </argument>
         </argumentList>
      </action>      
      <action>
         <name>PermitSpeak</name>
         <argumentList>
            <argument>
               <name>MemberName</name>
               <direction>in</direction>
               <relatedStateVariable>memberName</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>RaiseHand</name>
      </action>  
      <action>
         <name>SetLayoutType</name>
         <argumentList>
         	<argument>
         		<name>TypeName</name>
         		<direction>in</direction>
         		<relatedStateVariable>typeName</relatedStateVariable>
         	</argument>
         </argumentList>
      </action>  
      <action>
         <name>QuitMeeting</name>
         <argumentList>
         	<argument>
         		<name>MeetingName</name>
         		<direction>in</direction>
         		<relatedStateVariable>meetingName</relatedStateVariable>
         	</argument>
         </argumentList>
      </action>
   </actionList>
   <serviceStateTable>
      <stateVariable sendEvents="yes">
         <name>memberListChanged</name>
         <dataType>boolean</dataType>
      </stateVariable>
      <stateVariable sendEvents="yes">
         <name>beingInvited</name>
         <dataType>string</dataType>
      </stateVariable>
      <stateVariable sendEvents="yes">
         <name>beingKicked</name>
         <dataType>string</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>isAccept</name>
         <dataType>boolean</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>userAccount</name>
         <dataType>string</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>typeName</name>
         <dataType>string</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>memberName</name>
         <dataType>string</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>meetingName</name>
         <dataType>string</dataType>
      </stateVariable>      
      <stateVariable sendEvents="no">
         <name>memberList</name>
         <dataType>string</dataType>
      </stateVariable>
   </serviceStateTable>
</scpd>